
<div class="container">
  <div class="row">
    <div class="col-xs-4">
      <h1 class="text-center">Cocktails</h1>
      <% @cocktails.each do |cocktail| %>
      <ul>
        <h3><%= cocktail.name %></h3>


        <p> <%= link_to "See details", cocktail_path(cocktail) %> </p>


      </ul>
      <% end %>

      <%= link_to "Add new cocktail", new_cocktail_path %>
    </div>

  </div>

</div>
